{% extends 'base.html.twig' %}
{% block title %}details d'historique{% endblock %}
{% block body %}
    <div class="container">
    <input id="myInput" type="text" placeholder="filter">
    {{ include('readregistersandcoils/__historyalarm.html.twig') }}
    </div>
{% endblock %}
{% block javascripts %}
    <script>
        $(document).ready(function(){
            $("#myInput").on("keyup", function() {
                var value = $(this).val().toLowerCase();
                $("#tableh tr").filter(function() {
                    $(this).toggle($(this).text().toLowerCase().indexOf(value) > -1)
                });
            });
        });
    </script>
{% endblock %}